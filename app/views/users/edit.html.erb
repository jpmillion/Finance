<% if current_user.errors.any? %>
    <div id="error_explanation">
        <ul>
            <% current_user.errors.full_messages.each do |message| %>
                <li><%= message %></li>
            <% end %>
        </ul>
    </div>
<% end %>

<h1>Edit Profile</h1>

<%= form_with model: current_user do |f| %>
<%= render partial: 'form', locals: { user: current_user, f: f }%>
<% end %>